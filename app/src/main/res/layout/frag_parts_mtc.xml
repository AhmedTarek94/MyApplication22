<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.example.ltc_pc.myapplication.frag_parts_mtc"
    tools:layout_editor_absoluteY="25dp">

    <!-- TODO: Update blank fragment layout -->

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="54dp"
        android:layout_marginStart="3dp"
        android:layout_marginTop="16dp"
        android:background="@android:drawable/editbox_background_normal"
        android:text="Participants From the MTC"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/de1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/de1"
        android:layout_width="368dp"
        android:layout_height="0dp"
        android:layout_marginBottom="31dp"
        android:clickable="true"
        android:onClick="onClick"
        android:src="@drawable/elec"
        app:layout_constraintBottom_toTopOf="@+id/de2"
        app:layout_constraintEnd_toStartOf="@+id/de3"
        app:layout_constraintStart_toStartOf="@+id/de3"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <ImageView
        android:id="@+id/de2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="17dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:src="@drawable/mecha"
        android:clickable="true"
        android:onClick="onClick"
        app:layout_constraintBottom_toTopOf="@+id/de4"
        app:layout_constraintEnd_toStartOf="@+id/de3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/de1" />

    <ImageView
        android:id="@+id/de3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="8dp"
        android:src="@drawable/chemical_engineering1"
        android:clickable="true"
        android:onClick="onClick"
        app:layout_constraintBottom_toBottomOf="@+id/de2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/de2"
        app:layout_constraintTop_toTopOf="@+id/de2" />

    <ImageView
        android:id="@+id/de5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="8dp"
        android:src="@drawable/naval"
        android:clickable="true"
        android:onClick="onClick"
        app:layout_constraintBottom_toBottomOf="@+id/de4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/de4"
        app:layout_constraintTop_toTopOf="@+id/de4" />

    <ImageView
        android:id="@+id/de4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="35dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:src="@drawable/civil"
        android:clickable="true"
        android:onClick="onClick"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/de5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/de2" />

</android.support.constraint.ConstraintLayout>
